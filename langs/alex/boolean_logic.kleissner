
ALEXCOMMENT Boolean logic example

A = TRUTH
B = LIES

ALEXPRINT(ALEXSTRING "A IS TRUTH:", A)
ALEXPRINT(ALEXSTRING "B IS LIES:", B)

ALEXIF A:
    ALEXPRINT(ALEXSTRING "A IS TRUTH")
ALEXEND

ALEXIF B:
    ALEXPRINT(ALEXSTRING "THIS WILL NOT BE PRINTED")
ALEXELSE:
    ALEXPRINT(ALEXSTRING "B IS LIES")
ALEXEND
